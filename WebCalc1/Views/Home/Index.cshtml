@{
    ViewBag.Title = "Home Page";
}
<p>@Html.ActionLink("Create New", "Create")</p>
<table class="table">
    <thead>
        <tr>
            <th></th>
            <th>Id</th>
            <th>Фамилия</th>
            <th>Логин</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in ViewBag.Users)
            {
            <tr>
                <td></td>
                <td>@user.Id</td>
                <td><a href="Find?Id=@user.Id">@user.FIO</a></td>
                <td>@user.Login</td>
                <th><a href="Home/Delete?Id=@user.Id">Удалить</a></th>
            </tr>
        }
    </tbody>
</table>

